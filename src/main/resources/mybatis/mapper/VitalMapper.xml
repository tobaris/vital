<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
        namespace="vital.mapper.VitalMapper">

    <select id="find" resultType="VitalModel">
        SELECT
            id,
            name,
            price,
            type
        FROM
            item
